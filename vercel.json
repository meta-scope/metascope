{
  "builds": [
    {
      "use": "@vercel/python",
      "entrypoint": "python manage.py runserver",
      "config": {
        "maxLambdaSize": "20mb"
      },
      "env": {
        "DJANGO_SETTINGS_MODULE": "myproject.settings"
      },
      "dependencies": {
        "install": [
          "pip install -r requirements.txt"
        ]
      }
    }
  ]
}